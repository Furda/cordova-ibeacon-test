<?xml version='1.0' encoding='utf-8'?>
<widget id="com.furda.hello" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>cordova-ibeacon-test</name>
    <description>
        iBeacon app test
    </description>
    <author email="furda.dev@gmail.com" href="https://fernandourdaneta.dev/">
       Fernando Urdaneta
    </author>
    <content src="index.html" />
    <access origin="*" />
